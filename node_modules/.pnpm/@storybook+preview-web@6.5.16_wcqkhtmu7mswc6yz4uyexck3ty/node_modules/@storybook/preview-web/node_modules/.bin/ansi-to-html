#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eplougoulm/Desktop/lemonway/onboarding/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/eplougoulm/Desktop/lemonway/onboarding/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ansi-to-html@0.6.15/node_modules/ansi-to-html/bin/ansi-to-html" "$@"
else
  exec node  "$basedir/../../../../../../ansi-to-html@0.6.15/node_modules/ansi-to-html/bin/ansi-to-html" "$@"
fi
